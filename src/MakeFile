#for conveinence, define variable $(OBJS) to list object files
OBJS= mainUI.o tokenizer.o history.o tester.o
# compile flags. -g for debug, -03 for optimization
CFLAGS =-g -03

#first target is built by default
all: tokens

# include file dependencies
tokenizer.o: tokenizer.h


# $@= target, $^=prerequisites
tokens: $(OBJS)
	cc -o tokens $(OBJS) $(CFLAGS)

#deletes files generated by compilation
clean:
	rm -f *.o src
run: tokens
	./tokens

